<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Version>9.0.1</Version>
        <Authors>Anillen</Authors>
        <PackageOutputPath>Package</PackageOutputPath>
        <PackAsTool>False</PackAsTool>
        <Description>https://github.com/anillen/Anillen.ResultObjects</Description>
        <RepositoryUrl>https://github.com/anillen/Anillen.ResultObjects</RepositoryUrl>
        <RepositoryType>github</RepositoryType>
    </PropertyGroup>
    <Target Name="PublishPackageToServer" AfterTargets="Pack">
        <Exec Command="dotnet nuget push -s RadoNuget -k UkIN55aMadhBeWZ6F9eyTmDiWXGttS $(PackageOutputPath)/$(PackageId).$(PackageVersion).nupkg --skip-duplicate"/>
    </Target>
</Project>
